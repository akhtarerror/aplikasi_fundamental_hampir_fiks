<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="bangkit.mobiledev.aplikasidicodingeventsfix.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_upcoming"
        android:name="bangkit.mobiledev.aplikasidicodingeventsfix.ui.upcoming.UpComingFragment"
        android:label="@string/up_coming"
        tools:layout="@layout/fragment_up_coming" />
    <fragment
        android:id="@+id/navigation_finished"
        android:name="bangkit.mobiledev.aplikasidicodingeventsfix.ui.finished.FinishedFragment"
        android:label="@string/finished"
        tools:layout="@layout/fragment_finished" />
</navigation>